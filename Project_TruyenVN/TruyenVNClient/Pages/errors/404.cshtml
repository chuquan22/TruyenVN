
@page
@model TruyenVNClient.Pages.errors._404Model
@{
    ViewData["title"] = "Page does not exists";
    Layout = null;
}

<head>
    <link href="https://st.truyenqqq.vn/template/frontend/styles/style.css?v=1.8.8" type="text/css" rel="stylesheet">
</head>
<center>
<div style="margin-top: 5%">
    <div class="misc-wrapper">
        <h2 class="mb-2 mx-2">Page Not Found :(</h2>
        <p class="mb-4 mx-2">Oops! 😖 The requested URL was not found on this server.</p>
        <a href="/Index" class="btn btn-warning">Back to home</a>
        <div class="mt-3">
            <img src="/img/page-misc-error-light.png"
                 alt="page-misc-error-light"
                 width="500"
                 class="img-fluid"
                 data-app-dark-img="illustrations/page-misc-error-dark.png"
                 data-app-light-img="illustrations/page-misc-error-light.png" />
        </div>
    </div>
</div>
</center>